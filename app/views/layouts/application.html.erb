<!DOCTYPE html>
<html>
<head>
  <title>Fortuneteller</title>
  <%= stylesheet_link_tag "application" %>
  <%= javascript_include_tag "application" %>
  <%= yield :head %>
  <%= csrf_meta_tags %> 
</head>

<body>
     <%= render partial: "shared/main_menu" %>
     <% if notice %>  
       <p class="notice">
          <%= notice %>
       </p>
     <% end %>
     <% if alert %>
       <p class="alert"><%= alert %></p>
     <% end %>
<div class="container">
<%= yield %>
</div>
</body>
</html>


